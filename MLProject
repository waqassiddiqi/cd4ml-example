name: cd4ml-example

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      alpha: {type: float, default: 0.5}
      l1_ratio: {type: float, default: 0.1}
    command: "python code/pre_process.py && python code/train.py {alpha} {l1_ratio}"
  evaluate:
    parameters:
      run_id: {type: string}
    command: "python code/evaluate.py {run_id}"

